{
    "name": "setebit/setebit-package",
    "description": "",
    "keywords": [
        "setebit",
        "common-package-setebit"
    ],
    "homepage": "https://github.com/setebit/setebit-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Setebit",
            "email": "setebitdev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.2",
        "owen-it/laravel-auditing": "^13.0",
        "php-amqplib/php-amqplib": "^3.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Setebit\\Package\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Setebit\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Setebit\\Package\\SetebitPackageServiceProvider"
            ],
            "aliases": {
                "AuthData": "Setebit\\Package\\Facades\\AuthData",
                "UpdateBalance": "Setebit\\Package\\UpdateBalance"
            }
        }
    }
}
